extends ../layouts/noheader
include ../../blocks/modules/menu-left/menu-left
include ../../blocks/modules/header-search/header-search
include ../../blocks/modules/header-menu/header-menu
include ../../blocks/modules/header-profile/header-profile
include ../../blocks/modules/header-wallet/header-wallet
include ../../blocks/modules/replace/replace-item/replace-item
include ../../blocks/modules/replace/replace-action/replace-action.pug


block variables
  include ../../data/replace 



block aside
  +menu-left()

block pa-header
  +header-menu()
  +header-search()
  +header-profile()
  +header-wallet()

block pa-action
  +replace-action([
    'Подменные парковки',
    '/',
  ])

block pa-main
  .replace-parking
    .replace-parking__list
      .replace-parking__item
        each item in replace
          +replace-item(item)

block scripts

  script(src="js/main.js?v=1.70")
  script(src="js/vendor.js")
